{
  "name": "zero-ui",
  "version": "0.0.1",
  "description": "Library of OneZero used React components",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "@onezerocompany/zero-ui"
  },
  "keywords": [
    "ui",
    "framework",
    "components"
  ],
  "author": "OneZero Company",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-icons": "^4.8.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@onezerocompany/eslint-config": "^1.43.0",
    "@onezerocompany/prettier-config": "^1.43.0",
    "@onezerocompany/tsconfig": "^1.43.0",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-webpack5": "^7.0.2",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react": "^18.0.33",
    "prop-types": "^15.8.1",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.2",
    "typescript": "^5.0.3"
  },
  "prettier": "@onezerocompany/prettier-config",
  "eslintConfig": {
    "extends": "@onezerocompany/eslint-config/react",
    "parserOptions": {
      "project": "./tsconfig.json",
      "ecmaVersion": 2022
    },
    "rules": {
      "react/require-default-props": "off",
      "@typescript-eslint/naming-convention": "off"
    },
    "ignorePatterns": [
      "dist/",
      "node_modules/"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "React": true,
      "JSX": true
    }
  },
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  }
}
