#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/storybook@7.0.2/node_modules/storybook/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/storybook@7.0.2/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/storybook@7.0.2/node_modules/storybook/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/storybook@7.0.2/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../storybook/index.js" "$@"
else
  exec node  "$basedir/../storybook/index.js" "$@"
fi
