#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@storybook+semver@7.3.2/node_modules/@storybook/semver/bin/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@storybook+semver@7.3.2/node_modules/@storybook/semver/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@storybook+semver@7.3.2/node_modules/@storybook/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@storybook+semver@7.3.2/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@storybook+semver@7.3.2/node_modules/@storybook/semver/bin/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@storybook+semver@7.3.2/node_modules/@storybook/semver/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@storybook+semver@7.3.2/node_modules/@storybook/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@storybook+semver@7.3.2/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/semver.js" "$@"
else
  exec node  "$basedir/../../bin/semver.js" "$@"
fi
