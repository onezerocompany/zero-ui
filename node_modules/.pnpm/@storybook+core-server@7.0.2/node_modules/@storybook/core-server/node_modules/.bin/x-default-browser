#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.88/node_modules/@aw-web-design/x-default-browser/bin/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.88/node_modules/@aw-web-design/x-default-browser/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.88/node_modules/@aw-web-design/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.88/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.88/node_modules/@aw-web-design/x-default-browser/bin/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.88/node_modules/@aw-web-design/x-default-browser/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.88/node_modules/@aw-web-design/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/@aw-web-design+x-default-browser@1.4.88/node_modules:/Users/lucasilverentand/GitHub/zero-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@aw-web-design+x-default-browser@1.4.88/node_modules/@aw-web-design/x-default-browser/bin/x-default-browser.js" "$@"
else
  exec node  "$basedir/../../../../../../@aw-web-design+x-default-browser@1.4.88/node_modules/@aw-web-design/x-default-browser/bin/x-default-browser.js" "$@"
fi
